---
import AntlerChangelog100 from "../content/changelog/antler/antler-1.0.0.astro";
import AntlerChangelog120 from "../content/changelog/antler/antler-1.2.0.astro";
import AntlerChangelog130 from "../content/changelog/antler/antler-1.3.0.astro";
import AntlerChangelog140 from "../content/changelog/antler/antler-1.4.0.astro";
---

<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>Moose Antler Changelog</title>
    <link rel="icon" type="image/png" href="/images/logo.png" />
  </head>
  <body class="bg-gray-50 text-gray-800 font-inter">
    <main class="container mx-auto px-6 py-16">
      <h1 class="text-4xl font-bold mb-6">Moose Antler Changelog</h1>
      <p class="mb-12 text-gray-600">
        Keep up with all the new features, improvements, and fixes.
      </p>
      <div class="max-w-3xl mx-auto space-y-8">
        <AntlerChangelog140 />
        <AntlerChangelog130 />
        <AntlerChangelog120 />
        <AntlerChangelog100 />
      </div>
    </main>
  </body>
</html>
